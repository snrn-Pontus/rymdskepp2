<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module PUBLIC "-//Google Inc.//DTD Google Web Toolkit 2.8.0//EN" "https://gwt.googlesource.com/gwt/+/2.8.0/distro-source/core/src/gwt-module.dtd">
<module rename-to="html">
    <source path="" />
    <inherits name="se.snrn.rymdskepp.Rymdskepp" />
    <inherits name="se.snrn.rymdskepp.Shared" />
    <inherits name="com.badlogic.gdx.backends.gdx_backends_gwt" />
    <inherits name="Box2DLights" />
    <inherits name="com.badlogic.ashley_gwt" />
    <inherits name="com.badlogic.gdx.ai" />
    <inherits name="com.badlogic.gdx.controllers.controllers-gwt" />
    <inherits name="com.badlogic.gdx.physics.box2d.box2d-gwt" />
    <inherits name="com.github.czyzby.websocket.GdxWebSocketGwt" />
    <inherits name="com.github.czyzby.websocket.GdxWebSocketSerialization" />

    <inherits name="com.kotcrab.vis.vis-ui" />
    <inherits name="com.strongjoshua.console" />
    <inherits name="libgdx-utils" />
    <inherits name="libgdx-utils-box2d" />

    <entry-point class="se.snrn.rymdskepp.gwt.GwtLauncher" />
    <extend-configuration-property name="gdx.reflect.include" value="se.snrn.rymdskepp.components" />
    <extend-configuration-property name="gdx.reflect.include" value="se.snrn.rymdskepp.Assets" />
    <extend-configuration-property name="gdx.reflect.include" value="box2dLight.Spinor" />
    <extend-configuration-property name="gdx.reflect.include" value="com.badlogic.gdx.graphics.g2d.TextureAtlas.AtlasRegion"/>
    <set-configuration-property name="gdx.assetpath" value="../assets" />
    <set-configuration-property name="xsiframe.failIfScriptTag" value="FALSE"/>
    <set-property name="user.agent" value="safari"/>

</module>